  "version": "1.1.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "author": "SuitedCoder",
  "license": "MIT",
  "devDependencies": {
    "electron": "^20.2.0",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "electron-packager": "^16.0.0"
  },
  "build": {
    "appId": "com.suited.uranium",
    "productName": "Uranium Store",
